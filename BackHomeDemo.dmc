
HSPEED=1
ZATHOME=0
DPHMZ=0
POSZ=0
MINOFF=10
CKZ=0

#HOMEZ
IF(_LRC=1)
	IF(_MOC=1)
		SH C
	ENDIF
	AC ,,1000000
	DC ,,1000000
	IF(HSPEED)
		SP ,,100000
	ELSE 
		SP ,,30000
	ENDIF
	HM C
	BG C
	AM C
	ZATHOME=1	
ENDIF
EN

#DPHOMEZ
IF(DPHMZ=0)
	AC ,,1000000
	DC ,,1000000
	SP ,,30000
	FE C
	BG C
	AM C
	DP ,,0
	DPHMZ=1
ENDIF
EN


#CKHMZ
	IF(_SCC=9)|(_SCC=10)
		POSZ=_TPC
		IF(POSZ<=MINOFF)
			CKZ=1
		ELSE
			CKZ=0
			JP #HOMEZ
		ENDIF
	ENDIF
EN








